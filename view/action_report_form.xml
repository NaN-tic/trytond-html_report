<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook/page[@id='general']" position="after">
        <page string="HTML Report" id="html_report">
            <label name="html_template"/>
            <field name="html_template"/>
            <label name="html_header_template"/>
            <field name="html_header_template"/>
            <label name="html_footer_template"/>
            <field name="html_footer_template"/>
            <label name="html_last_footer_template"/>
            <field name="html_last_footer_template"/>
            <label name="html_raise_user_error"/>
            <field name="html_raise_user_error"/>
            <separator name="html_templates" colspan="4"/>
            <field name="html_templates" colspan="4"/>
            <separator name="html_content" colspan="4"/>
            <field name="html_content" colspan="4"/>
        </page>
        <page string="Jinja Template" id="jinja_template">
            <field name="jinja_template" colspan="4"/>
        </page>
    </xpath>
</data>
